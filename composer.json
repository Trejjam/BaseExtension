{
	"name": "trejjam/base-extension",
	"description": "Simple Nette extension helper",
	"keywords": [
		"nette",
		"base extension"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Trejbal"
		}
	],
	"require": {
		"php": ">=7.1",
		"nette/di": "~2.3 || ^3.0@beta",
		"nette/utils": "~2.3 || ^3.0@beta"
	},
